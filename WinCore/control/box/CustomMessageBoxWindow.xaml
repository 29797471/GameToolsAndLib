<Window x:Class="WinCore.CustomMessageBoxWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinCore"
        mc:Ignorable="d"
        Title="CustomMessageBoxWindow" x:Name="self" MinHeight="100" MinWidth="100"  SizeToContent="WidthAndHeight" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox FontSize="20" Text="{Binding ElementName=self,Path=DataContext}" IsEnabled="True" IsReadOnly="True" AcceptsReturn="True" AcceptsTab="True"/>
        
    </Grid>
</Window>
